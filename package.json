{
  "name": "@bugfender/rn-bugfender",
  "version": "2.1.0",
  "description": "React Native bindings for Bugfender SDK",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "README.md",
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "RnBugfender.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "bugfender",
    "logging",
    "debug"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bugfender/rn-bugfender.git",
    "baseUrl": "https://github.com/bugfender/rn-bugfender.git"
  },
  "author": {
    "name": "Bugfender"
  },
  "license": "ISC",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/bugfender/rn-bugfender/issues"
  },
  "homepage": "https://github.com/bugfender/rn-bugfender.git#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@react-native-community/eslint-config": "^3.1.0",
    "@release-it/conventional-changelog": "^5.1.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.18",
    "@types/react-native": "0.69.6",
    "commitlint": "^17.1.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "pod-install": "^0.1.38",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-native": "0.69.5",
    "react-native-builder-bob": "^0.18.3",
    "release-it": "^15.4.1",
    "typescript": "^4.8.2"
  },
  "peerDependencies": {
    "react": "*",
    "yar": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "off"
    },
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "rules": {
          "no-dupe-class-members": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "@bugfender/common": "1.0.0-alpha.0",
    "@bugfender/sdk": "^2.1.0"
  }
}
